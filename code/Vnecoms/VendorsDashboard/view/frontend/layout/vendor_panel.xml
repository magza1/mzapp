<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
    	<attribute name="class" value="account"/>
        <referenceBlock name="seller.menu">
            <block class="Vnecoms\Vendors\Block\Seller\Menu\Item" name="seller.menu.dashboard" before="-">
				<arguments>
                    <argument name="label" xsi:type="string" translate="true">Dashboard</argument>
                    <argument name="path" xsi:type="string">marketplace/dashboard</argument>
                    <argument name="html_class" xsi:type="string">dashboard</argument>
                    <argument name="icon_class" xsi:type="string">fa-dashboard</argument>
                </arguments>
            </block>
        </referenceBlock>
        <move element="page.main.title" destination="content.top" before="-"/>
    </body>
</page>
