<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
        <css src="Vnecoms_VendorsCredit::css/styles.css" />
    </head>
    <body>
	    <referenceContainer name="left">
            <block class="Vnecoms\VendorsCredit\Block\Adminhtml\Withdraw\View\Tabs" name="view_withdrawal_tabs">
                <block class="Vnecoms\VendorsCredit\Block\Adminhtml\Withdraw\View\Tab\Main" name="view_withdrawal_tab_main" />
                <action method="addTab">
                    <argument name="name" xsi:type="string">main_section</argument>
                    <argument name="block" xsi:type="string">view_withdrawal_tab_main</argument>
                </action>
            </block>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Vnecoms\VendorsCredit\Block\Adminhtml\Withdraw\View" name="view_withdrawal"/>
        </referenceContainer>
    </body>
</page>
