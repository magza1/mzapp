<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Vnecoms_VendorsMessage::css/font-awesome.min.css"/>
        <css src="Vnecoms_VendorsMessage::css/message.css" />
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Vnecoms\VendorsMessage\Block\Adminhtml\Messages\View" name="messages.all" template="view.phtml">

                <arguments>
                    <argument name="block_title" xsi:type="string">View Message</argument>
                </arguments>
                <block class="Vnecoms\VendorsMessage\Block\Adminhtml\Messages\View" name="messages.list" as="message_content" template="view/list.phtml" />

            </block>
        </referenceContainer>
    </body>
</page>
