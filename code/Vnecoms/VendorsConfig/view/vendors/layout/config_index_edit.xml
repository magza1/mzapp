<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="vendor-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="js">
            <block class="Magento\Backend\Block\Template" template="Vnecoms_VendorsConfig::system/config/js.phtml"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Vnecoms\VendorsConfig\Block\System\Config\Tabs" name="adminhtml.system.config.tabs" before="-"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block name="system.config.edit" class="Vnecoms\VendorsConfig\Block\System\Config\Edit"/>
        </referenceContainer>
    </body>
</page>
