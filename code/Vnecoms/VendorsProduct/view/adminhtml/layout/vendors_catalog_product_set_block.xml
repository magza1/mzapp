<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<update handle="catalog_product_set_block"/>
    <body>
        <referenceBlock name="adminhtml.catalog.product.set.grid.columnSet">
        	<arguments>
        		<argument name="rowUrl" xsi:type="array">
                    <item name="path" xsi:type="string">vendors/*/edit</item>
                    <item name="extraParamsTemplate" xsi:type="array">
                        <item name="id" xsi:type="string">getAttributeSetId</item>
                    </item>
                </argument>
        	</arguments>
        </referenceBlock>
    </body>
</page>
