<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="seller.menu">
            <block class="Vnecoms\Vendors\Block\Seller\Menu\Item" name="seller.menu.catalog">
				<arguments>
                    <argument name="label" xsi:type="string" translate="true">Catalog</argument>
                    <argument name="html_class" xsi:type="string">catalog</argument>
                    <argument name="icon_class" xsi:type="string">fa-cubes</argument>
                </arguments>
                
                <block class="Vnecoms\Vendors\Block\Seller\Menu\Item" name="seller.menu.catalog.product">
					<arguments>
	                    <argument name="label" xsi:type="string" translate="true">Manage Products</argument>
	                    <argument name="path" xsi:type="string">marketplace/catalog_product/</argument>
	                </arguments>
	            </block>
            </block>
        </referenceBlock>
    </body>
</page>
