<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="top.links">
            <block class="Vnecoms\Vendors\Block\Account\Link" name="seller-dashboard-link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Seller Dashboard</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="header.links">
            <block class="Vnecoms\Vendors\Block\Account\Create\Link" name="seller-register-link" after="register-link">
            	<arguments>
                    <argument name="label" xsi:type="string" translate="true">Seller Account</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
